[run]
branch = True
omit =
    entity_event/migrations/*
    entity_event/south_migrations/*
    entity_event/tests/*
    entity_event/version.py
    entity_event/apps.py
source = entity_event
[report]
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain if tests don't hit defensive assertion code:
    raise NotImplementedError
fail_under = 100
show_missing = 1
